.esquerda {
    float: left;
    overflow: hidden;
    width: 540px;
    margin: 0 10px 0 0;
}
.right {
    float: left;
}

.clear-left {
    clear: left;
}
.clear-esquerda {
    clear: left;
}

.borda-fix {
    border: 1px solid #94adc2;
    overflow: hidden;
    padding: 5px;
    clear: both;
}

.combo-check {
    width: 250px;
  
   
    height: 200px;
    float: left;
    overflow: hidden;
    overflow-y: scroll;
    border: 1px solid #94adc2;
}
.combo-check ul {
    width: 240px;
    padding: 5px !important;
    overflow: hidden;
}
.combo-check ul li {
    overflow: hidden;
}
.combo-check ul li label {
    width: 200px;
    float: left;
}
.combo-check ul li input {   
    width: 20px;
    float: left;
}


.small-input {
    width: 35px;
}

.campo {
	float: left;
	margin: 5px 10px 5px 0px;
	padding: 0px;
}

	.campo span {
		margin: 0px;
		padding: 0px;
		font-size: 10px;
		font-weight: bold;
		color: #a47e3c;
	}

