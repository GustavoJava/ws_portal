.realupload {
	position: absolute;
	float: left;
	margin-top: 3px;
	left: 450px;
	opacity: 0;
	filter: alpha(opacity : 0);
	height: 50px;
}

.fakeupload {
	width: 500px;
	cursor: default;
	background-color: #FFFFFF;
}

select{
	background-color: #FFFFFF;
	border: 1px solid #e4e4e4;
	letter-spacing: 1px;
	font-size: 11px;
	color: #333;
	margin-top: 10px;
	margin-bottom: 15px;
}

input{
	background-color: #FFFFFF;
	border: 1px solid #e4e4e4;
	letter-spacing: 1px;
	font-size: 11px;
	color: #333;
	padding-left: 5px;
	padding-top: 5px;
	padding-bottom: 5px;
	--margin-left: 5px;
	vertical-align: middle;
	margin-top: 10px;
	margin-bottom: 15px;
}

#tipo_vei{
	margin-top: 15px;
}


/* JANELA MODAL */
/* O z-index do div#mask deve ser menor que do div#boxes e do div.window */
#mask {
	position:absolute;
	z-index:9000;  
	background-color:#000; 
	display:none;
}

#boxes .window {
	position:absolute;
	display:none;
	z-index:9999;
	padding:20px;
	background-color:#FFF; 
}

/* Personalize a janela modal aqui. Você pode adicionar uma imagem de fundo. */
#boxes #dialog {
	width:375px;
	height:150px;
	--border: 1px solid #000;
}

/* FIM JANELA MODAL*/