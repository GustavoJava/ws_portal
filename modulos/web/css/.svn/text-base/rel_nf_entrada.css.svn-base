.highlight {
	background-color: #FFFFC0;
}

h3{
    margin-left: 0px;
}

.ui-widget-content{
    border: 0;
}

.ui-widget-header {
    background-image: none;
    background-color: #FFFFFF;
    border: 0;
}

.ui-corner-top{
    border-top-right-radius: 0;
    border-top-left-radius: 0;
}

#tabs .ui-tabs-nav{
    padding: 0;
}

#tabs{
    width: 98%;
}

#tabs .panel{
    width: 100%;
    background-color: #FFFFFF;
    border: 1px solid #94ADC2;
    clear: left;
    margin: 0 0 10px;
    padding: 0;
    text-align: left;
    vertical-align: top;
}

#tabs .ui-tabs-panel{
    padding: 0;
}

#tabs .ui-state-default {
    background-image: url("../../../images/fundo.gif");
}

#tabs .ui-tabs-selected {
    background-image: url("../../../images/fundo_over3.jpg");
}

.left{
