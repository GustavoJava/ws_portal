.botao {
    cursor: pointer;
    margin: 0px 5px;
    padding: 1px 5px;
    font-size: 12px;
    border: 1px solid #999;
    background: url('../../../images/fd_tr_principal.gif');
    text-decoration: none;
    color: #000;
    font-family: Arial;
}
.botao:hover {
    text-decoration: none;
}

.left {
    width: 45%;
    float: left;
    clear: left;
    padding: 0 0 10px 0;
}
.right {
    width: 45%;
    float: right;
    clear: right;
    padding: 0 0 10px 0;
}

dl.dados-rescisao {
    font-size: 11px;
    overflow: hidden;
    padding: 10px 10px;
}
dl.dados-rescisao dt {
    width: 100%;
    font-weight: bold;
    float: left;
    clear: left;    
    padding: 0;
    /*height: 20px;*/
}
dl.dados-rescisao dd {
    float: left;
    margin: 0 0 3px 0;
    /*height: 20px;*/
    width: 65%;
    padding: 0;
}
dl.dados-rescisao dd input{
    width: 120px;
}
dl.dados-rescisao dd input,
dl.dados-rescisao dd select {
    *margin: 0;
    margin: 15px 0 0 0;
}
dl.dados-rescisao dd div.data{
    *margin: 0;
    margin: 15px 0 0 0;
}
#cliente_form {
    display: inline;
}
.container-loader {
    width: 100%;
    position: relative;
    height: 50px;
    display: none;
}
.loader {
    background-image: url('../images/loading.gif');
    background-position: center;
    *background-repeat: no-repeat;
    width: 65px;
    *width: 100%;
    height: 50px;
    display: block;
    margin: 0 auto;
}
.ie7 .container-loader .loader {
    background-image: url('../images/loading.gif');
    background-position: center;
    width: 90%;
    height: 50px;
    display: block;
    margin: 0 auto;
    position: absolute;
    top: 0;
    left: 50%;
    margin-left: -33px;
    background-repeat: no-repeat;
}
input[readonly="readonly"] {
    background-color: #f2f2f2;
}
input,
select,
textarea {
    border: 1px solid #94adc2;
}
.highlight {
    background-color: #ffffc0;
}
table tr.totalizador {
    background-color: #BAD0E5 !important;
    font-weight: bold;
}
.loader-finalizacao {
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: 999999 !important;
    background: rgb(255, 255, 255) transparent;
    background: rgba(255, 255, 255, 0.6);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#99FFFFFF, endColorstr=#99FFFFFF);
    -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#99FFFFFF, endColorstr=#99FFFFFF)";
    display: none;
}
.overlay img {
    position: absolute;
    left: 50%;
    bottom: 20px;
    margin: 0 0 0 -50px;
}
.col-center {
    text-align: center !important;
}
/*.botao.busca-cliente {
    position: relative;
    top: 2px;
}*/
.data-max {
    width: 120px;
}
.data-max input.campo {
    width: 83px;
}
.data-max img {
    float: right;
    margin: 3px 8px 0 0;
    padding: 0px;
    width: 25px;
    height: 20px;
    border: 0px;
}
.data-max input.campo {
  padding: 0 2px;
}
#cliente_form #cliente {
    /*width: 150px !important;*/
}
#data_inicial,
#data_final {
    margin-left: 0 !important;
}
.range {
    color: #000 !important;
    font-weight: normal !important;
    font-size: 11px !important;
    width: 5px !important;
}
.ie7 .left input,
.ie7 .right input {
    margin-left: 0 !important;
}
div.listagem {
    overflow: hidden !important;
}
/* Estilos carta */
table.tabela {
    border-collapse: collapse;
}
table.tabela tr td,
table.tabela tr th {
    border: 1px solid #000000;
    padding: 5px;
}
table.tabela tr th {
    font-weight: bold;
}
strong {
    font-weight: bold;
}
.pesquisaMaior{
    width: 385px !important;
}
