jQuery(document).ready(function(){
    
    
    //Botão novo da página de pesquisa.
    jQuery('#bt_confirmar').click(function(e){
        
        e.preventDefault(); 
        
        
        //Defina a ação
        jQuery('#acao').val('cadastrar');
        
        //Envia o formulário
        jQuery('#form').submit();
        
    });
    
});